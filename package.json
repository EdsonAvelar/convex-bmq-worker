  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@upstash/redis": "^1.34.3",
    "bullmq": "^5.63.0",
    "ioredis": "^5.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "prisma": "^6.16.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
